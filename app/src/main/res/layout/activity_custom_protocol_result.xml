<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_main">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.15">

        <TextView
            android:text="NovelT"
            android:textColor="@color/white"
            android:textSize="30sp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <View
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/text_time_custom_protocol_result"
            android:textColor="@color/white"
            android:text="2019. 02. 12. PM 2:12"
            android:textSize="30sp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15dp"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="0.3"
            android:layout_height="match_parent"
            android:background="@drawable/background_content"
            android:orientation="vertical">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:text="ID :"
                    android:textColor="#bebebe"
                    android:textSize="25sp"
                    android:layout_width="0dp"
                    android:layout_weight="0.3"
                    android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/text_userid_custom_protocol_result"
                    android:text="jaehyeok"
                    android:textSize="27sp"
                    android:textColor="@color/white"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content" />

            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:text="BAND :"
                    android:textColor="#bebebe"
                    android:textSize="20sp"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/text_patch_custom_protocol_result"
                    android:text="9150"
                    android:textColor="#bebebe"
                    android:textSize="20sp"
                    android:gravity="center"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:visibility="invisible" />

                <ImageView
                    android:id="@+id/indicator_connection_custom_protocol_result"
                    android:src="@drawable/img_connect"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="-5dp"
                    android:layout_width="0dp"
                    android:layout_weight="0.2"
                    android:layout_height="wrap_content"
                    android:visibility="invisible" />

                <ImageView
                    android:id="@+id/indicator_battery_custom_protocol_result"
                    android:src="@drawable/battery_indicator_50p"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="-70dp"
                    android:layout_width="0dp"
                    android:layout_weight="0.2"
                    android:layout_height="wrap_content"
                    android:visibility="invisible" />

            </LinearLayout>

            <TextView
                android:text="Protocol :"
                android:textColor="#bebebe"
                android:textSize="25sp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="-40dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/text_protocol_custom_protocol_result"
                android:textColor="@color/white"
                android:textSize="25sp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <!--<TextView
                android:text="Rank :"
                android:textColor="#bebebe"
                android:textSize="25sp"
                android:layout_marginTop="0dp"
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/text_rank_custom_protocol_result"
                android:text="0 / 542"
                android:textColor="#bebebe"
                android:textSize="25sp"
                android:layout_marginTop="0dp"
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />-->

            <Button
                android:id="@+id/btn_out_custom_protocol_result"
                android:text="나가기"
                android:textColor="#bebebe"
                android:textSize="20sp"
                android:background="@drawable/btn_out"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="40dp"
                android:layout_width="wrap_content"
                android:layout_height="50dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:background="@drawable/background_content_mile_test">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">

                        <TextView
                            android:text="Time"
                            android:textColor="#bebebe"
                            android:textSize="20sp"
                            android:gravity="center"
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />

                        <ProgressBar
                            android:id="@+id/custom_protocol_result_progressbar"
                            style="?android:attr/progressBarStyleHorizontal"
                            android:indeterminate="false"
                            android:max="100"
                            android:progress="20"
                            android:layout_centerInParent="true"
                            android:progressDrawable="@drawable/circular_progress"
                            android:secondaryProgress="100"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent" />

                        <TextView
                            android:id="@+id/text_progress_custom_protocol_result"
                            android:textColor="@color/white"
                            android:text="0%"
                            android:textSize="20sp"
                            android:layout_alignLeft="@id/custom_protocol_result_progressbar"
                            android:layout_alignRight="@id/custom_protocol_result_progressbar"
                            android:layout_alignTop="@id/custom_protocol_result_progressbar"
                            android:layout_alignBottom="@id/custom_protocol_result_progressbar"
                            android:gravity="center"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />

                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <TextView
                            android:text="Heart Rate"
                            android:textColor="#bebebe"
                            android:textSize="20sp"
                            android:gravity="center"
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />

                        <RelativeLayout
                            android:id="@+id/custom_protocol_result_HRBar"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginTop="10dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="10dp"
                            android:layout_marginBottom="10dp">

                            <TextView
                                android:id="@+id/text_hr_max_custom_result"
                                android:text="000"
                                android:textColor="@color/white"
                                android:textSize="15sp"
                                android:layout_alignParentBottom="true"
                                android:layout_marginBottom="20dp"
                                android:layout_marginLeft="62dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content" />

                            <TextView
                                android:id="@+id/text_hr_avg_custom_result"
                                android:text="000"
                                android:textColor="@color/white"
                                android:textSize="15sp"
                                android:layout_alignParentBottom="true"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="40dp"
                                android:layout_marginBottom="20dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content" />

                        </RelativeLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <TextView
                            android:text="@string/speed"
                            android:textColor="#bebebe"
                            android:textSize="20sp"
                            android:gravity="center"
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />

                        <RelativeLayout
                            android:id="@+id/custom_protocol_result_SpeedBar"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginTop="10dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="10dp"
                            android:layout_marginBottom="10dp">

                            <TextView
                                android:id="@+id/text_speed_max_custom_result"
                                android:text="000"
                                android:textColor="@color/white"
                                android:textSize="15sp"
                                android:layout_alignParentBottom="true"
                                android:layout_marginBottom="20dp"
                                android:layout_marginLeft="62dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content" />

                            <TextView
                                android:id="@+id/text_speed_avg_custom_result"
                                android:text="000"
                                android:textColor="@color/white"
                                android:textSize="15sp"
                                android:layout_alignParentBottom="true"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="40dp"
                                android:layout_marginBottom="20dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content" />

                        </RelativeLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:layout_marginBottom="20dp">

                        <TextView
                            android:text="Distance"
                            android:textColor="#bebebe"
                            android:textSize="25sp"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="10dp"
                            android:layout_width="wrap_content"
                            android:layout_height="0dp"
                            android:layout_weight="1"/>

                        <TextView
                            android:id="@+id/text_distance_custom_protocol_result"
                            android:background="@drawable/background_textview"
                            android:text="0 km"
                            android:textColor="@color/white"
                            android:textSize="25sp"
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="10dp"
                            android:layout_height="0dp"
                            android:layout_weight="1"/>

                        <TextView
                            android:text="Calorie"
                            android:textColor="#bebebe"
                            android:textSize="25sp"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="25dp"
                            android:layout_width="wrap_content"
                            android:layout_height="0dp"
                            android:layout_weight="1"/>

                        <TextView
                            android:id="@+id/text_calorie_custom_protocol_result"
                            android:background="@drawable/background_textview"
                            android:text="0 kcal"
                            android:textColor="@color/white"
                            android:textSize="25sp"
                            android:gravity="center"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"/>

                    </LinearLayout>

                </LinearLayout>

                <!--<RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="15dp">

                </RelativeLayout>-->

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="0.3"
            android:layout_height="match_parent"
            android:background="@drawable/background_content_custom_protocol"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="5dp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="0.7"
                android:orientation="horizontal"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp">

                <ImageView
                    android:src="@drawable/img_tropy"
                    android:layout_gravity="center"
                    android:layout_margin="1dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <TextView
                    android:text="Today's Rank"
                    android:textColor="#bebebe"
                    android:textSize="20sp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="10dp">

                <ImageView
                    android:src="@drawable/img_tropy_gold"
                    android:layout_gravity="center"
                    android:layout_margin="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginRight="10dp">

                    <TextView
                        android:id="@+id/text_username_rank_gold"
                        android:text="노 종 민"
                        android:textColor="@color/white"
                        android:textSize="17sp"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1" />

                    <TextView
                        android:id="@+id/text_userlocation_rank_gold"
                        android:text="강북지점"
                        android:textColor="@color/white"
                        android:textSize="13sp"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.8" />

                    <TextView
                        android:id="@+id/text_userdistance_rank_gold"
                        android:text="4.12 km"
                        android:textColor="@color/white"
                        android:textSize="13sp"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.8" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="10dp">

                <ImageView
                    android:src="@drawable/img_tropy_sliver"
                    android:layout_gravity="center"
                    android:layout_margin="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginRight="10dp">

                    <TextView
                        android:id="@+id/text_username_rank_silver"
                        android:text="황 은 진"
                        android:textColor="@color/white"
                        android:textSize="17sp"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1" />

                    <TextView
                        android:id="@+id/text_userlocation_rank_silver"
                        android:text="부평지점"
                        android:textColor="@color/white"
                        android:textSize="13sp"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.8" />

                    <TextView
                        android:id="@+id/text_userdistance_rank_silver"
                        android:text="4.10 km"
                        android:textColor="@color/white"
                        android:textSize="13sp"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.8" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="10dp">

                <ImageView
                    android:src="@drawable/img_tropy_cooper"
                    android:layout_gravity="center"
                    android:layout_margin="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginRight="10dp">

                    <TextView
                        android:id="@+id/text_username_rank_cooper"
                        android:text="차 병 준"
                        android:textColor="@color/white"
                        android:textSize="17sp"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1" />

                    <TextView
                        android:id="@+id/text_userlocation_rank_cooper"
                        android:text="수원지점"
                        android:textColor="@color/white"
                        android:textSize="13sp"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.8" />

                    <TextView
                        android:id="@+id/text_userdistance_rank_cooper"
                        android:text="4.02 km"
                        android:textColor="@color/white"
                        android:textSize="13sp"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.8" />

                </LinearLayout>

            </LinearLayout>

            <ImageView
                android:src="@drawable/img_rank_divider"
                android:layout_width="match_parent"
                android:layout_height="10.5dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="0.4"
                android:orientation="horizontal"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="5dp">

                <TextView
                    android:text="나의 순위 : "
                    android:textColor="@color/white"
                    android:textSize="20sp"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/text_user_rank_custom_result"
                    android:text="123위"
                    android:textColor="@color/white"
                    android:textSize="20sp"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content" />

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/background_hr_graph"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/text_tab_hr_graph_custom_protocol_result"
                android:background="@drawable/btn_tab"
                android:text="Heart Rate Graph (bpm)"
                android:textColor="#bebebe"
                android:textSize="25sp"
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <!--<TextView
                android:id="@+id/text_tab_normal_distribution_custom_protocol_result"
                android:background="@drawable/btn_tab"
                android:text="Normal Distribution"
                android:textColor="#bebebe"
                android:textSize="25sp"
                android:gravity="center"
                android:layout_marginLeft="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />-->

        </LinearLayout>

        <FrameLayout
            android:id="@+id/tab_framelayout_custom_protocol_result"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="20dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <com.exam.novelt3_1.Custom_HRZoneGraph
                android:id="@+id/HRGraph_custom_result"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

        </FrameLayout>

    </LinearLayout>

    <TextView
        android:text="Copyright 2019. Hanyang Digitech all rights reserved."
        android:textColor="#ffff00"
        android:textSize="13sp"
        android:gravity="end"
        android:layout_marginBottom="10dp"
        android:layout_marginRight="25dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</LinearLayout>